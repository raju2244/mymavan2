pipeline
{
    agent any
    stages
    {
        stage('continuousDownload_Loans')
        {
            steps 
        {
           git 'https://github.com/raju2244/mymavan2.git'
        }
            
        }
         stage('continuousBuild_Loans')
         {
             steps
             {
                sh 'mvn package'
             }
         }
        
        
    }
    
}
